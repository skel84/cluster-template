# IMPORTANT: This file is not tracked by Argo and should never be. Its
# purpose is to only install the Argo components and CRDs into your cluster.
---
apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization

resources:
  - ./namespace.yaml
  - github.com/argoproj/argo-cd/manifests/cluster-install?ref=v2.9.18

patches:
  - path: ./argocd-cm.yaml
  - path: ./argocd-repo-server.yaml
